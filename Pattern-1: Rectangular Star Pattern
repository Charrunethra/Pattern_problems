//Pattern 1
/*
Input: N = 3
Output: 
* * *
* * *
* * *
*/
import java.util.*;
public class Main
{
    public static void pattern1(int n){
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                System.out.print("* ");
            }
            System.out.println();
        }
    }
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		pattern1(n);
	}
}


//in c++;
#include <iostream> //#include<bits/stdc++.h>

using namespace std;

void pattern1(int n){
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            cout<<"* ";
        }
        cout<<endl;
    }
}
int main()
{
    int n;
    cin>>n;
    pattern1(n);

    return 0;
}
